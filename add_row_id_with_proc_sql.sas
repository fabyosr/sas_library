DATA TMP_DATAS;
FORMAT DT_COMPRA 8.;
INFORMAT DT_COMPRA $8.;
INPUT DT_COMPRA;
DATALINES;
20180101
20180201
20180301
20180401
;
RUN;
PROC SQL;
%LET ROW_ID = 0 ;
CREATE TABLE TMP_ROW_ID AS
SELECT INPUT(RESOLVE('%LET ROW_ID=%EVAL(&ROW_ID+1); &ROW_ID'),32.)AS ROW_ID,
       A.*
FROM TMP_DATAS A;
%SYMDEL ROW_ID;
QUIT ;